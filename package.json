    "chaos:pod-kill": "./scripts/chaos-test.sh pod-kill",
    "chaos:latency": "./scripts/chaos-test.sh latency",
    "chaos:report": "./scripts/chaos-test.sh report",
    "chaos:cleanup": "./scripts/chaos-test.sh cleanup",
    "chaos:all": "./scripts/chaos-test.sh all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sarvarjafarov/ads-data.git"
  },
